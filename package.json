{
  "name": "create-chic",
  "version": "0.0.3",
  "packageManager": "pnpm@6.32.3",
  "description": "template manager for creating new projects.",
  "keywords": [
    "cli",
    "create",
    "manager",
    "templates"
  ],
  "homepage": "https://github.com/banlify/create-chic#readme",
  "bugs": {
    "url": "https://github.com/banlify/create-chic/issues"
  },
  "author": "chestnut <banlify@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banlify/create-chic.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "esno ./scripts/build.ts dev",
    "build": "rimraf dist && zx ./scripts/build.mjs",
    "lint": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^9.1.0",
    "degit": "^2.8.4",
    "fs-extra": "^10.0.1",
    "minimist": "^1.2.6",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "cz-customizable": "^6.3.0",
    "esbuild": "^0.14.35",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.7.10",
    "zx": "^6.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
